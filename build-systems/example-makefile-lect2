# Note: this file does not comply with the format, as it uses spaces for indentation. Why? because it is not meant to be run.


say-hello
    @echo "World
    @echo "hello"


silent-gcc
    @gcc *.c -o somecoolname
    
non-silent-gcc
    gcc *.c -o someOtherName


with-no-dependencies:
    gcc myprogram.c -o myprogram


with-dependencies: mypprogram.c myprogram.h
    gcc mypprogram.c -o myprogram


clean: 
    rm myprogram -rf *.o

